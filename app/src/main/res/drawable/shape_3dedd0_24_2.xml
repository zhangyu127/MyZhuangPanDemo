<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- rectangle表示为矩形 -->

    <!-- 填充的颜色 -->
    <solid android:color="#FFFFFF" />

    <!-- 边框的颜色和粗细 -->
    <stroke
        android:width="4px"
        android:color="#3DEDD0" />

    <!-- android:radius 圆角的半径 -->
    <corners android:radius="48px" />
</shape>
